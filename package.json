{
  "name": "npm-publish-parser",
  "version": "0.1.0",
  "description": "Parse the output of `npm publish --dry-run` into JSON",
  "keywords": [
    "npm",
    "publish",
    "dry-run",
    "json"
  ],
  "author": "legend80s",
  "type": "module",
  "files": [
    "lib/index.mjs",
    "lib/lite-lodash.mjs",
    "index.d.ts",
    "index.mjs"
  ],
  "types": "index.d.ts",
  "bin": {
    "npm-publish-json": "index.mjs"
  },
  "main": "index.mjs",
  "scripts": {
    "pub:patch": "npm version patch",
    "pub:minor": "npm version minor",
    "pub:major": "npm version major",
    "preversion": "npm run cov",
    "postversion": "npm publish && git push && git push --tags",
    "cov": "node --test --experimental-test-coverage",
    "test": "node --test"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legend80s/npm-publish-json.git"
  },
  "bugs": {
    "url": "https://github.com/legend80s/npm-publish-json/issues"
  },
  "homepage": "https://github.com/legend80s/npm-publish-json#readme",
  "license": "MIT"
}
